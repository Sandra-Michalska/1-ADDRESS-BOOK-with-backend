div(class="address-div bg-gray relative-margin", id=address.rowid)

    p.address-div__text.address-div-name= address.name
    p.address-div__text.address-div-phone= address.phone
    p.address-div__text.address-div-address= address.address
    p.address-div__text Groups

    input(type="image", src="img/edit.jpg", alt="edit", class="button-edit outline-none")
    input(type="image", src="img/delete.jpg", alt="delete", class="button-delete outline-none")
