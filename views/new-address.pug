div(class="address-div bg-gray relative-margin", id=addressData.rowid)

    p.address-div__text.address-div-name= addressData.name
    p.address-div__text.address-div-phone= addressData.phone
    p.address-div__text.address-div-address= addressData.address
    p.address-div__text Groups

    input(type="image", src="img/edit-small.png", alt="edit", class="button-edit outline-none")
    input(type="image", src="img/delete-small.png", alt="delete", class="button-delete outline-none")
